package com.hackerrank;

import java.util.Scanner;

public class JavaLoops {
	Scanner in = new Scanner(System.in);
	int t=in.nextInt();
	
	for(int i = 0;i<t;i++)
	{
		int a = in.nextInt();
		int b = in.nextInt();
		int n = in.nextInt();
		int num = 1;
		int sum = a;
		for (int j = 1; j <= n; j++) {
			sum = num * b;
			System.out.println(sum);
			num = num * 2;
		}
	}in.close();
}}
